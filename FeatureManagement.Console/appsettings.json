{
  "Logging": {
    "Console": {
      "IncludeScopes": true
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FeatureManagement": {
    "VerboseLogging": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "End": "01/08/2023 12:00:00"
          }
        }
      ]
    },
    "AddressLogging": {       // Feature name
      "EnabledFor": [
        {
          "Name": "PersonAddress",    // Filter Condition (without Filter suffix
          "Parameters": {
            "Address": "Famous Street"
          }
        }
      ]
    }
  }
}
